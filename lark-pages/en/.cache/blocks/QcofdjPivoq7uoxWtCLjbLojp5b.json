ements":[{"text_run":{"content":"Whale CRM addresses the following issues:","text_element_style":{"bold":false,"inline_code":false,"italic":false,"strikethrough":false,"underline":false}}}],"style":{"align":1,"folded":false}}},{"block_id":"doxjpvPysEYT2VxkBdLcHf9jvzd","block_type":13,"ordered":{"elements":[{"text_run":{"content":"Scattered customer information: Basic customer data, KYC documentation, and business data are dispersed across Excel spreadsheets, text files, and email; there is no single repository that stores all customer information.","text_element_style":{"bold":false,"inline_code":false,"italic":false,"strikethrough":false,"underline":false}}}],"style":{"align":1,"folded":false}},"parent_id":"QcofdjPivoq7uoxWtCLjbLojp5b"},{"block_id":"doxjpGjNAsqK9DNsXHWCe2F8o8d","block_type":13,"ordered":{"elements":[{"text_run":{"content":"Cumbersome team collaboration: Cross-team cooperation is inefficient; there are no standardized processes.","text_element_style":{"bold":false,"inline_code":false,"italic":false,"strikethrough":false,"underline":false}}}],"style":{"align":1,"folded":false}},"parent_id":"QcofdjPivoq7uoxWtCLjbLojp5b"},{"block_id":"doxjpBuMVT0wigUVLS0bUYc5Udd","block_type":13,"ordered":{"elements":[{"text_run":{"content":"Lagging risk monitoring: No continuous real-time risk monitoring mechanism has been established; the organization cannot effectively fulfill regulatory obligations or take timely measures.","text_element_style":{"bold":false,"inline_code":false,"italic":false,"strikethrough":false,"underline":false}}}],"style":{"align":1,"folded":false}},"parent_id":"QcofdjPivoq7uoxWtCLjbLojp5b"},{"block_id":"doxjpBZNmkDPaROpBnW99fNxr9c","block_type":13,"ordered":{"elements":[{"text_run":{"content":"Manual-dependent business management: Task assignment and reporting are not automated; managerial efficiency is low.","text_element_style":{"bold":false,"inline_code":false,"italic":false,"strikethrough":false,"underline":false}}}],"style":{"align":1,"folded":false}},"parent_id":"QcofdjPivoq7uoxWtCLjbLojp5b"},{"block_id":"doxjpiU7oNopcsJlPUEWRYxrI2g","block_type":2,"parent_id":"QcofdjPivoq7uoxWtCLjbLojp5b","text":{"elements":[{"text_run":{"content":"In the core account opening process, the Whale CRM system is designed to synchronize the three phases—pre-, mid-, and post-account opening—providing comprehensive management of account opening activities:","text_element_style":{"bold":false,"inline_code":false,"italic":false,"strikethrough":false,"underline":false}}}],"style":{"align":1,"folded":false}}},{"block_id":"doxjpACV6WIcEiyKRDX7SW8AY0b","block_type":12,"bullet":{"elements":[{"text_run":{"content":"Pre-account opening: Management of the progression from registered user to active account (CRM management)","text_element_style":{"bold":false,"inline_code":false,"italic":false,"strikethrough":false,"underline":false}}}],"style":{"align":1,"folded":false}},"parent_id":"QcofdjPivoq7uoxWtCLjbLojp5b"},{"block_id":"doxjpk0WnNTP53dQv4Kph9yOPVc","block_type":12,"bullet":{"elements":[{"text_run":{"content":"During account opening: KYC review process after account documentation submission (KYC process)","text_element_style":{"bold":false,"inline_code":false,"italic":false,"strikethrough":false,"underline":false}}}],"style":{"align":1,"folded":false}},"parent_id":"QcofdjPivoq7uoxWtCLjbLojp5b"},{"block_id":"doxjph8qSiJHbQi2YUAUeLrz32b","block_type":12,"bullet":{"elements":[{"text_run":{"content":"Post-account opening: Ticketing management for account security after account opening (Account management)","text_element_style":{"bold":false,"inline_code":false,"italic":false,"strikethrough":false,"underline":false}}}],"style":{"align":1,"folded":false}},"parent_id":"QcofdjPivoq7uoxWtCLjbLojp5b"},{"block_id":"doxjpK17JJ7MEGu5jbeFFCA7eBh","block_type":2,"parent_id":"QcofdjPivoq7uoxWtCLjbLojp5b","text":{"elements":[{"text_run":{"content":"The overall business flow of Whale CRM is shown in the diagram below:","text_element_style":{"b