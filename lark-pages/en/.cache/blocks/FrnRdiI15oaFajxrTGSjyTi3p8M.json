hrough":false,"underline":false}}}],"style":{"align":1,"folded":false}}},{"block_id":"doxjpmqn1ReKp2OosJfzGTQjUag","block_type":19,"callout":{"background_color":5,"border_color":5,"emoji_id":"gift","text_color":1},"children":["doxjpYJGcJMPGJSxwoGS7qXHYbf","doxjphjeryGE6WmEZoJ2H1RCVsg"],"parent_id":"FrnRdiI15oaFajxrTGSjyTi3p8M"},{"block_id":"doxjpYJGcJMPGJSxwoGS7qXHYbf","block_type":2,"parent_id":"doxjpmqn1ReKp2OosJfzGTQjUag","text":{"elements":[{"text_run":{"content":"","text_element_style":{"bold":false,"inline_code":false,"italic":false,"strikethrough":false,"underline":false}}}],"style":{"align":1,"folded":false}}},{"block_id":"doxjphjeryGE6WmEZoJ2H1RCVsg","block_type":2,"parent_id":"doxjpmqn1ReKp2OosJfzGTQjUag","text":{"elements":[{"text_run":{"content":"\"Actual Received Amount\" = \"Receivable Amount\" - \"Deduction Amount\"","text_element_style":{"bold":false,"inline_code":false,"italic":false,"strikethrough":false,"underline":false}}}],"style":{"align":1,"folded":false}}},{"block_id":"doxjpVVGqCRtcD6sVMGxt32bAGd","block_type":27,"image":{"align":2,"height":1306,"scale":1,"token":"PgZKbByNGocra3x49P5jSzm9pnd","width":2688},"parent_id":"FrnRdiI15oaFajxrTGSjyTi3p8M"},{"block_id":"doxjpdKFmAOFwozXTYjouY2iBgu","block_type":3,"heading1":{"elements":[{"text_run":{"content":"5.3D Q: If a back-end supplemental order has more than one transaction record, how should the \"Back-End Order Number\" and \"Back-End Transaction Serial Number\" be filled in?","text_element_style":{"bold":false,"inline_code":false,"italic":false,"strikethrough":false,"underline":false}}}],"style":{"align":1,"folded":false}},"parent_id":"FrnRdiI15oaFajxrTGSjyTi3p8M"},{"block_id":"doxjpbdHMugogefBDSYsVX29k9g","block_type":2,"parent_id":"FrnRdiI15oaFajxrTGSjyTi3p8M","text":{"elements":[{"text_run":{"content":"A: Enter the same contract number for the \"Back-End Order Number\"; enter different serial numbers for the \"Back-End Transaction Serial Number.\"","text_element_style":{"bold":false,"inlin