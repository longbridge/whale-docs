ne_code":false,"italic":false,"strikethrough":false,"underline":false}}},{"text_run":{"content":"sidebar_position: 5","text_element_style":{"bold":false,"inline_code":false,"italic":false,"strikethrough":false,"underline":false}}}],"style":{"language":67,"wrap":false}},"parent_id":"Ke4Vdq415oHE91xTCyMjJgfRprh"},{"block_id":"doxjpRgxVnVd6S6OT722KJdi7oe","block_type":3,"heading1":{"elements":[{"text_run":{"content":"6. Corporate Action Questions","text_element_style":{"bold":false,"inline_code":false,"italic":false,"strikethrough":false,"underline":false}}}],"style":{"align":1,"folded":false}},"parent_id":"Ke4Vdq415oHE91xTCyMjJgfRprh"},{"block_id":"doxjpdBWlU1KhSuq4wH11YZpcdc","block_type":3,"heading1":{"elements":[{"text_run":{"content":"6A Q: What do the different corporate action types mean? (e.g., categories BE, CA, RS, TM)","text_element_style":{"bold":false,"inline_code":false,"italic":false,"strikethrough":false,"underline":false}}}],"style":{"align":1,"folded":false}},"parent_id":"Ke4Vdq415oHE91xTCyMjJgfRprh"},{"block_id":"doxjplJAusrnAGgAFZAT85iX4Zg","block_type":2,"parent_id":"Ke4Vdq415oHE91xTCyMjJgfRprh","text":{"elements":[{"text_run":{"content":"A: See the list below describing the different corporate action categories.","text_element_style":{"bold":false,"inline_code":false,"italic":false,"strikethrough":false,"underline":false}}}],"style":{"align":1,"folded":false}}},{"block_id":"doxjpdnwY4ec18X8CmEOYU5G24e","block_type":19,"callout":{"background_color":5,"border_color":5,"emoji_id":"gift","text_color":1},"children":["doxjpaesQwa5Zd1x2W0utWH2Peb","doxjp9NBuatqih4OBMqWRKPQ5hc"],"parent_id":"Ke4Vdq415oHE91xTCyMjJgfRprh"},{"block_id":"doxjpaesQwa5Zd1x2W0utWH2Peb","block_type":2,"parent_id":"doxjpdnwY4ec18X8CmEOYU5G24e","text":{"elements":[{"text_run":{"content":"","text_element_style":{"bold":false,"inline_code":false,"italic":false,"strikethrough":false,"underline":false}}}],"style":{"align":1,"folded":false}}},{"block_id":"doxjp9NBuatqih4OBMqWRKPQ5hc","block_type":2,"parent_id":"doxjpdnwY4ec18X8CmEOYU5G24e","text":{"elements":[{"text_run":{"content":"After a corporate action is completed, it generally takes about 5 minutes for the cost price to be updated.","text_element_style":{"bold":false,"inline_code":false,"italic":false,"strikethrough":false,"underline":false}}}],"style":{"align":1,"folded":false}}},{"block_id":"doxjp4q7EX9PMjhBS8SkrcHqMgc","block_type":30,"parent_id":"Ke4Vdq415oHE91xTCyMjJgfRprh","sheet":{"token":"Hx6rsDshDhzhnhtsQgHjJLqPpgd_anTIaj"}},{"block_id":"doxjp6UUYCe5VznOpwrlkZ7rXxg","block_type":3,"heading1":{"elements":[{"text_run":{"content":"6B. Q: How are corporate actions for US equities handled?","text_element_style":{"bold":false,"inline_code":false,"italic":false,"strikethrough":false,"underline":false}}}],"style":{"align":1,"folded":false}},"parent_id":"Ke4Vdq415oHE91xTCyMjJgfRprh"},{"block_id":"doxjpzuIigrSviIcENBaHpFtZGg","block_type":2,"parent_id":"Ke4Vdq415oHE91xTCyMjJgfRprh","text":{"elements":[{"text_run":{"content":"A: US corporate actions should be handled according to the instructions provided by upstream.","text_element_style":{"bold":false,"inline_code":false,"italic":false,"strikethrough":false,"underline":false}}}],"style":{"align":1,"folded":false}}},{"block_id":"doxjp8U12OnSbfC6n0BqLpyPC9c","block_type":12,"bullet":{"elements":[{"text_run":{"content":"For BE category actions, refer to Quick Start - Regular Cash Dividend (BE) and Quick Start - Voluntary Cash Dividend (BE).","text_element_style":{"bold":false,"inline_code":false,"italic":false,"strikethrough":false,"underline":false}}}],"style":{"align":1,"folded":false}},"parent_id":"Ke4Vdq415oHE91xTCyMjJgfRprh"},{"block_id":"doxjpcp2sMVrnp5NjshrgMjZdWf","block_type":19,"callout":{"background_color":5,"border_color":5,"emoji_id":"gift","text_color":1},"children":["doxjpszarHGc9eQ88IHCRmqzt7c","doxjpVmDOQYdvTW3Gvr7t4Lzx4d"],"parent_id":"Ke4Vdq415oHE91xTCyMjJgfRprh"},{"block_id":"doxjpszarHGc9eQ88IHCRmqzt7c","block_type":2,"parent_id":"doxjpcp2sMVrnp5NjshrgMjZdWf","text":{"elements":[{"text_run":{"content":"","text_element_style":{"bold":false,"inline_code":false,"italic":false,"strikethrough":false,"underline":false}}}],"style":{"align":1,"folded":false}}},{"block_id":"doxjpVmDOQYdvTW3Gvr7t4Lzx4d","block_type":2,"parent_id":"doxjpcp2sMVrnp5NjshrgMjZdWf","text":{"elements":[{"text_run":{"content":"If a CA-type corporate action occurs for US equities, the US end-of-day “clearing and settlement” must be completed to pull the latest data.","text_element_style":{"bold":false,"inline_code":false,"italic":false,"strikethrough":false,"underline":false}}},{"text_run":{"content":"For example: NVDA carried out a 1-for-10 share consolidation after market close on June 7, 2024. The system must complete the US clearing and settlement on June 10; the CA corporate action’s record date will be June 10 and the payment date will be June 11.","text_element_style":{"bold":false,"inline_code":false,"italic":false,"strikethrough":false,"underline":false}}}],"style":{"align":1,"folded":false}}},{"block_id":"doxjpTTywz43EfAGFyLKvd3xfhg","block_type":3,"heading1":{"elements":[{"text_run":{"content":"6C Q: Why are there no client records after I clicked \"Register\"?","text_element_style":{"bold":false,"inline_code":false,"italic":false,"strikethrough":false,"underline":false}}}],"style":{"align":1,"folded":false}},"parent_id":"Ke4Vdq415oHE91xTCyMjJgfRprh"},{"block_id":"doxjpmR50XHUCH5zFLmfp6s7RWd","block_type":2,"parent_id":"Ke4Vdq415oHE91xTCyMjJgfRprh","text":{"elements":[{"text_run":{"content":"A: If the equity record date is April 8, the system must complete the end-of-day processing for that date. Only after the system generates data can you click \"Register\" to retrieve client information. Clicking \"Register\" prematurely will return no data.","text_element_style":{"bold":false,"inline_code":false,"italic":false,"strikethrough":false,"underline":false}}}],"style":{"align":1,"folded":false}}},{"block_id":"doxjpHT7jYgEZmAotFcRZE2MaWW","block_type":2,"parent_id":"Ke4Vdq415oHE91xTCyMjJgfRprh","text":{"elements":[{"text_run":{"content":"If you need to complete the corporate action before the end of day, you can change the equity record date to April 7, then click \"Register\" again to pull client data.","text_element_style":{"bold":false,"inline_code":false,"italic":false,"strikethrough":false,"underline":false}}}],"style":{"align":1,"folded":false}}},{"block_id":"doxjpNoq1kCDaQTLYkLRSAB4F4f","block_type":27,"image":{"align":2,"height":849,"token":"QimCbDXZCoXtUtxESXxjvjXopHe","width":2131},"parent_id":"Ke4Vdq415oHE91xTCyMjJgfRprh"},{"block_id":"doxjpKgCHM3MI7PVGTpVb7fJmFd","block_type":3,"heading1":{"elements":[{"text_run":{"content":"6D Q: What if the distributed dividend amount differs from the system amount?","text_element_style":{"bold":false,"inline_code":false,"italic":false,"strikethrough":false,"underline":false}}}],"style":{"align":1,"folded":false}},"parent_id":"Ke4Vdq415oHE91xTCyMjJgfRprh"},{"block_id":"doxjp9mnuLwWEpuIFk6OtLmxvse","block_type":2,"parent_id":"Ke4Vdq415oHE91xTCyMjJgfRprh","text":{"elements":[{"text_run":{"content":"A: You can perform a \"Residual Adjustment\" to input the actual distributed amount.","text_element_style":{"bold":false,"inline_code":false,"italic":false,"strikethrough":false,"underline":false}}}],"style":{"align":1,"folded":false}}},{"block_id":"doxjpu04PmOCcMmV5ZgMHqwOy8g","block_type":2,"parent_id":"Ke4Vdq415oHE91xTCyMjJgfRprh","text":{"elements":[{"text_run":{"content":"(i) On the \"Corporate Action\" page, click \"Details\" in the action column.","text_element_style":{"bold":false,"inline_code":false,"italic":false,"strikethrough":false,"underline":false}}}],"style":{"align":1,"folded":false}}},{"block_id":"doxjpSBFiiryOJUbUVAX9QOjXVf","block_type":27,"image":{"align":2,"height":1376,"token":"E0dubx5E7oA7lZxCmk1jaCfIpAe","width":2816},"parent_id":"Ke4Vdq415oHE91xTCyMjJgfRprh"},{"block_id":"doxjphTb2ZvdAT01olBo5GSN0Rc","block_type":2,"parent_id":"Ke4Vdq415oHE91xTCyMjJgfRprh","text":{"elements":[{"text_run":{"content":"(ii) In \"Detail Summary,\" click \"Residual Adjustment\" to select the amount/quantity, then click \"Submit for Execution.\"","text_element_style":{"bold":false,"inline_code":false,"italic":false,"strikethrough":false,"underline":false}}}],"style":{"align":1,"folded":false}}},{"block_id":"doxjpJuBr5L4SaKvyih40w9r0Mb","block_type":27,"image":{"align":2,"height":1236,"token":"MFgmbIsw8o71FrxrsyPjlbk4pud","width":2084},"parent_id":"Ke4Vdq415oHE91xTCyMjJgfRprh"},{"block_id":"doxjpayIMPHyeoAvKNC1LLbtbY2","block_type":3,"heading1":{"elements":[{"text_run":{"content":"6E Q: Can dividends from corporate actions be received in different currencies?","text_element_style":{"bold":false,"inline_code":false,"italic":false,"strikethrough":false,"underline":false}}}],"style":{"align":1,"folded":false}},"parent_id":"Ke4Vdq415oHE91xTCyMjJgfRprh"},{"block_id":"doxjpZre7YbU0l9Zb9V5gektuFb","block_type":2,"parent_id":"Ke4Vdq415oHE91xTCyMjJgfRprh","text":{"elements":[{"text_run":{"content":"A: In \"Scheme Information\" click \"Edit,\" add a \"BE-CASH DIVIDEND\" scheme, input the \"Exchange Rate\" and the \"Dividend Rate\" (the dividend per share), then click \"Re-register\" to pull the latest data.","text_element_style":{"bold":false,"inline_code":false,"italic":false,"strikethrough":false,"underline":false}}}],"style":{"align":1,"folded":false}}},{"block_id":"doxjpmBEfRMhFAUAOUHj4wbQ7Ud","block_type":27,"image":{"align":2,"height":1490,"token":"JEkQbolASoYO3oxnvpUjoTa6ptg","width":2236},"parent_id":"Ke4Vdq415oHE91xTCyMjJgfRprh"},{"block_id":"doxjpJufKP9hQb7mvwMPvtJK7Bf","block_type":3,"heading1":{"elements":[{"text_run":{"content":"6F Q: How to subscribe for or modify a client’s Rights Subscription exercise quantity?","text_element_style":{"bold":false,"inline_code":false,"italic":false,"strikethrough":false,"underline":false}}}],"style":{"align":1,"folded":false}},"parent_id":"Ke4Vdq415oHE91xTCyMjJgfRprh"},{"block_id":"doxjpGnhCdgixCqJRMSUT1LYIIf","block_type":2,"parent_id":"Ke4Vdq415oHE91xTCyMjJgfRprh","text":{"elements":[{"text_run":{"content":"A: (i) After \"Register\" succeeds, you can view the exercisable shares and client quantities in the \"Detail Summary\" section. Add clients in \"Client Details\" or edit them in \"Details.\"","text_element_style":{"bold":false,"inline_code":false,"italic":false,"strikethrough":false,"underline":false}}}],"style":{"align":1,"folded":false}}},{"block_id":"doxjpr9vBgeE4xssqG0NEihrAQd","block_type":27,"image":{"align":2,"height":1392,"token":"N0tRby1FXox9pTx3SkUjITuWp9g","width":2367},"parent_id":"Ke4Vdq415oHE91xTCyMjJgfRprh"},{"block_id":"doxjpgSV5KrNBUWxa3hMaEvNaod","block_type":2,"parent_id":"Ke4Vdq415oHE91xTCyMjJgfRprh","text":{"elements":[{"text_run":{"content":"(ii) In \"Client Details\" you will see eligible clients for distribution. After modifying data, click \"Re-register\" to refresh. Once data is confirmed, click \"Exercise Instruction Collection\" to proceed.","text_element_style":{"bold":false,"inline_code":false,"italic":false,"strikethrough":false,"underline":false}}}],"style":{"align":1,"folded":false}}},{"block_id":"doxjphWuNiBN5Xeu0kA3b5aN8Ug","block_type":12,"bullet":{"elements":[{"text_run":{"content":"Click \"Add\": add an individual client to the distribution list.","text_element_style":{"bold":false,"inline_code":false,"italic":false,"strikethrough":false,"underline":false}}}],"style":{"align":1,"folded":false}},"parent_id":"Ke4Vdq415oHE91xTCyMjJgfRprh"},{"block_id":"doxjpBctoxwawSHovgkbbe4fuJe","block_type":12,"bullet":{"elements":[{"text_run":{"content":"Click \"Void\": remove a client.","text_element_style":{"bold":false,"inline_code":false,"italic":false,"strikethrough":false,"underline":false}}}],"style":{"align":1,"folded":false}},"parent_id":"Ke4Vdq415oHE91xTCyMjJgfRprh"},{"block_id":"doxjpsW3HC472dwNcavsDRcfdbh","block_type":12,"bullet":{"elements":[{"text_run":{"content":"Click \"Details\": modify a client’s charge information or entitlement transfer quantity.","text_element_style":{"bold":false,"inline_code":false,"italic":false,"strikethrough":false,"underline":false}}}],"style":{"align":1,"folded":false}},"parent_id":"Ke4Vdq415oHE91xTCyMjJgfRprh"},{"block_id":"doxjpVFAVk4LMOOvAHGeRuGo3Qd","block_type":27,"image":{"align":2,"height":1438,"token":"MOn7bi80SoOp6BxoZh4jkJOopJb","width":2391},"parent_id":"Ke4Vdq415oHE91xTCyMjJgfRprh"},{"block_id":"doxjpJML49FhIsMZomPbvU96mme","block_type":2,"parent_id":"Ke4Vdq415oHE91xTCyMjJgfRprh","text":{"elements":[{"text_run":{"content":"(iii) Under \"Exercise Information\" you can view a client’s exercise quantity; click \"Exercise\" to modify it.","text_element_style":{"bold":false,"inline_code":false,"italic":false,"strikethrough":false,"underline":false}}},{"text_run":{"content":"\"Charge Information\" shows the corporate action fees for the client; click \"Edit\" to adjust.","text_element_style":{"bold":false,"inline_code":false,"italic":false,"strikethrough":false,"underline":false}}},{"text_run":{"content":"\"Entitlement Information\" shows the distributed securities; click \"Edit\" to modify.","text_element_style":{"bold":false,"inline_code":false,"italic":false,"strikethrough":false,"underline":false}}}],"style":{"align":1,"folded":false}}},{"block_id":"doxjpOcimS2t7WBoG9mSKZ8VCOf","block_type":27,"image":{"align":2,"height":1390,"token":"Ozcdb52dDolvPtxJq6sjpqDkpoh","width":1849},"parent_id":"Ke4Vdq415oHE91xTCyMjJgfRprh"},{"block_id":"doxjphO82n4lFVA8M7BzP0b10ld","block_type":3,"heading1":{"elements":[{"text_run":{"content":"6G Q: If a client does not subscribe for their Rights Subscription (RS), will the system automatically remove it?","text_element_style":{"bold":false,"inline_code":false,"italic":false,"strikethrough":false,"underline":false}}}],"style":{"align":1,"folded":false}},"parent_id":"Ke4Vdq415oHE91xTCyMjJgfRprh"},{"block_id":"doxjps2ur37keC3EzjNbY7LTT9d","block_type":2,"parent_id":"Ke4Vdq415oHE91xTCyMjJgfRprh","text":{"elements":[{"text_run":{"content":"A: You must operate a DS-type corporate action to recall the equity.","text_element_style":{"bold":false,"inline_code":false,"italic":false,"strikethrough":false,"underline":false}}}],"style":{"align":1,"folded":false}}},{"block_id":"doxjpa9yG9HN1BJFkSmLh5TP5BC","block_type":2,"parent_id":"Ke4Vdq415oHE91xTCyMjJgfRprh","text":{"elements":[{"text_run":{"content":"RS handles the subscribed and reported entitlements; DS handles the unsubscribed entitlements. During CCASS processing, the two corporate actions have different record dates; DS-type actions will be generated slightly later.","text_element_style":{"bold":false,"inline_code":false,"italic":false,"strikethrough":false,"underline":false}}}],"style":{"align":1,"folded":false}}},{"block_id":"doxjpz87x4XaGTp5XepFAd8htjf","block_type":3,"heading1":{"elements":[{"text_run":{"content":"6H Q: When will the system remove knocked-out CBBCs?","text_element_style":{"bold":false,"inline_code":false,"italic":false,"strikethrough":false,"underline":false}}}],"style":{"align":1,"folded":false}},"parent_id":"Ke4Vdq415oHE91xTCyMjJgfRprh"},{"block_id":"doxjpMgfNt8PXRZexdagDiOGUSg","block_type":2,"parent_id":"Ke4Vdq415oHE91xTCyMjJgfRprh","text":{"elements":[{"text_run":{"content":"A: When creating a new corporate action, select TM or DS depending on whether residual value remains, then enter the record date provided by upstream in \"Record Date.\" When the issuer removes the product, complete the corporate action to reclaim the underlying.","text_element_style":{"bold":false,"inline_code":false,"italic":false,"strikethrough":false,"underline":false}}}],"style":{"align":1,"folded":false}}},{"block_id":"doxjpaMR7A9TqQuyRCZLZoqFXQw","block_type":27,"image":{"align":2,"height":1226,"token":"HCujbYERkoZcrQxfURGjzQI7pfL","width":2674},"parent_id":"Ke4Vdq415oHE91xTCyMjJgfRprh"},{"block_id":"doxjpDShLnSsBdLT7TRNlFhO5Cb","block_type":27,"image":{"align":2,"height":1126,"token":"EXXnbniSRo8ZOdxyXCujpAXKpub","width":904},"parent_id":"Ke4Vdq415oHE91xTCyMjJgfRprh"},{"block_id":"doxjp1XFfF9jEqRthpIhjjlWQAg","block_type":3,"heading1":{"elements":[{"text_run":{"content":"6I Q: What should I do if an error occurs when performing \"Exercise Instruction Collection\"?","text_element_style":{"bold":false,"inline_code":false,"italic":false,"strikethrough":false,"underline":false}}}],"style":{"align":1,"folded":false}},"parent_id":"Ke4Vdq415oHE91xTCyMjJgfRprh"},{"block_id":"doxjpVBoPNX09yR3nUiIZsnIF9d","block_type":27,"image":{"align":2,"height":510,"token":"RyrjbQarko2IahxFmibjiI1Opcc","width":2137},"parent_id":"Ke4Vdq415oHE91xTCyMjJgfRprh"},{"block_id":"doxjpZ7pvdPOY5xLvHjIxtt9Meb","block_type":2,"parent_id":"Ke4Vdq415oHE91xTCyMjJgfRprh","text":{"elements":[{"text_run":{"content":"A: When operating \"Exercise Instruction Collection,\" the selected Start Time and End Time must be in sequence. After success, you can click \"Complete Instruction Collection Early\" to finish the corporate action ahead of schedule.","text_element_style":{"bold":false,"inline_code":false,"italic":false,"strikethrough":false,"underline":false}}},{"text_run":{"content":"Additionally, the time intervals must progress sequentially: choose Start Time < choose End Time < choose End Time (CCASS).","text_element_style":{"bold":false,"inline_code":false,"italic":false,"strikethrough":false,"underline":false}}}],"style":{"align":1,"folded":false}}},{"block_id":"doxjpTMKiBCjOsm2z2ZX6cQ4gEf","block_type":3,"heading1":{"elements":[{"text_run":{"content":"6J Q: In which report are corporate actions displayed?","text_element_style":{"bold":false,"inline_code":false,"italic":false,"strikethrough":false,"underline":false}}}],"style":{"align":1,"folded":false}},"parent_id":"Ke4Vdq415oHE91xTCyMjJgfRprh"},{"block_id":"doxjpshNoiEkFlXUncMHCXpq0xh","block_type":2,"parent_id":"Ke4Vdq415oHE91xTCyMjJgfRprh","text":{"elements":[{"text_run":{"content":"A: You can view the SDR040 category reports. Corporate actions that are executed as \"mandatory\" distributions appear in SDR040-09 Corporate Action Detail Report (BE/IP/SO).","text_element_style":{"bold":false,"inline_code":false,"italic":false,"strikethrough":false,"underline":false}}},{"text_run":{"content":"Optional scrip/dividend selection corporate actions appear in SDR040-10 Corporate Action Detail Report (BE With Selection).","text_element_style":{"bold":false,"inline_code":false,"italic":false,"strikethrough":false,"underline":false}}}],"style":{"align":1,"folded":false}}},{"block_id":"doxjptLv3m59h60M0zC8VkMNvTb","block_type":3,"heading1":{"elements":[{"text_run":{"content":"6K Q: Which types of corporate actions must be reported to the Off‑Market Transfer Reporting regime (OTCR)?","text_element_style":{"bold":false,"inline_code":false,"italic":false,"strikethrough":false,"underline":false}}}],"style":{"align":1,"folded":false}},"parent_id":"Ke4Vdq415oHE91xTCyMjJgfRprh"},{"block_id":"doxjpRKfVajUKJJGTtmISzmTHGc","block_type":2,"parent_id":"Ke4Vdq415oHE91xTCyMjJgfRprh","text":{"elements":[{"text_run":{"content":"A: Corporate actions that are exempt from stamp duty do not need to be reported to the OTCR. Cash offer–type corporate actions must be uploaded; use the ","text_element_style":{"bold":false,"inline_code":false,"italic":false,"strikethrough":false,"underline":false}}},{"text_run":{"content":"SDR070 - OTCR Report","text_element_style":{"bold":false,"inline_code":true,"italic":false,"strikethrough":false,"underline":false}}},{"text_run":{"content":". Note: you must export to an Excel file to display takeover share transfer records — in‑page queries and CSV export do not support this.","text_element_style":{"bold":false,"inline_code":false,"italic":false,"strikethrough":false,"underline":false}}}],"style":{"align":1,"folded":false}}},{"block_id":"doxjpRVX48QMLMlzVmSi3yr1J3b","block_type":27,"image":{"align":2,"height":808,"token":"HnT3bYsQvo9oyoxpqGQjrFt1pPr","width":2042},"parent_id":"Ke4