e_code":false,"italic":false,"strikethrough":false,"underline":false}}},{"text_run":{"content":"sidebar_position: 2","text_element_style":{"bold":false,"inline_code":false,"italic":false,"strikethrough":false,"underline":false}}}],"style":{"language":67,"wrap":false}},"parent_id":"K3qdd1XEoor1t9xRePkjJNI8pah"},{"block_id":"doxjpqG2cEiWWAf7uX6rzkD2ALh","block_type":3,"heading1":{"elements":[{"text_run":{"content":"System Introduction","text_element_style":{"bold":false,"inline_code":false,"italic":false,"strikethrough":false,"underline":false}}}],"style":{"align":1,"folded":false}},"parent_id":"K3qdd1XEoor1t9xRePkjJNI8pah"},{"block_id":"doxjpB8sUduoYDRJG6GndepzsFc","block_type":4,"heading2":{"elements":[{"text_run":{"content":"I. System Overview","text_element_style":{"bold":false,"inline_code":false,"italic":false,"strikethrough":false,"underline":false}}}],"style":{"align":1,"folded":false}},"parent_id":"K3qdd1XEoor1t9xRePkjJNI8pah"},{"block_id":"doxjp4kKMPRZRCNedwY5GHchNec","block_type":2,"parent_id":"K3qdd1XEoor1t9xRePkjJNI8pah","text":{"elements":[{"text_run":{"content":"The securities deposit function refers to the operation by which investors transfer funds into their securities accounts via a securities trading platform. Investors can conveniently deposit funds into their securities accounts and participate in securities trading and investment activities anytime and anywhere.","text_element_style":{"bold":false,"inline_code":false,"italic":false,"strikethrough":false,"underline":false}}}],"style":{"align":1,"folded":false}}},{"block_id":"doxjpxolyjRPrMFelBzGY6vknig","block_type":2,"parent_id":"K3qdd1XEoor1t9xRePkjJNI8pah","text":{"elements":[{"text_run":{"content":"The Whale system includes features such as deposit applications, deposit details, streamlined import parameter settings, and email notifications to brokers. The workflow supports the approval requirements of different roles and effectively enhances the accuracy of securities deposits. The overall process architecture is as follows:","text_element_style":{"bold":false,"inline_code":false,"italic":false,"strikethrough":false,"underline":false}}}],"style":{"align":1,"folded":false}}},{"block_id":"Qxzjd7fImoowYIxaSaBjhubgpIb","block_type":43,"board":{"token":"QeAbwYwJqhgu5gbphrFjIHg2p7e"},"parent_id":"K3qdd1XEoor1t9xRePkjJNI8pah"},{"block_id":"doxjpvabzUR9xiJU0nqdrAqAyR3","block_type":4,"heading2":{"elements":[{"text_run":{"content":"II. Operating Instructions","text_element_style":{"bold":false,"inline_code":false,"italic":false,"strikethrough":false,"underline":false}}}],"style":{"align":1,"folded":false}},"parent_id":"K3qdd1XEoor1t9xRePkjJNI8pah"},{"block_id":"doxjpe8vHCJb2VwRWbA3fvE93Pg","block_type":19,"callout":{"background_color":5,"border_color":5,"emoji_id":"round_pushpin","text_color":1},"children":["doxjpXOaEIQqxbrW9T8z1hQGtdd"],"parent_id":"K3qdd1XEoor1t9xRePkjJNI8pah"},{"block_id":"doxjpXOaEIQqxbrW9T8z1hQGtdd","block_type":2,"parent_id":"doxjpe8vHCJb2VwRWbA3fvE93Pg","text":{"elements":[{"text_run":{"content":" Menu entry: ","text_element_style":{"bold":false,"inline_code":false,"italic":false,"strikethrough":false,"underline":false}}},{"text_run":{"content":"Stock Management - Broker Config","text_element_style":{"bold":false,"inline_code":false,"italic":false,"strikethrough":false,"underline":false}}}],"style":{"align":1,"folded":false}}},{"block_id":"doxjpRZKEPlwu71wiaeFh0Fhrgg","block_type":5,"heading3":{"elements":[{"text_run":{"content":"Deposit Application","text_element_style":{"bold":false,"inline_code":false,"italic":false,"strikethrough":false,"underline":false}}}],"style":{"align":1,"folded":false}},"parent_id":"K3qdd1XEoor1t9xRePkjJNI8pah"},{"block_id":"doxjpInYsLjWMEKR8uLePT1WuDc","block_type":2,"parent_id":"K3qdd1XEoor1t9xRePkjJNI8pah","text":{"elements":[{"text_run":{"content":"Securities deposit applications are initiated by the client and comprise four main sections: broker information, account information, security details, and remarks. WHALE users can manually assist clients with securities deposit processing and perform actions such as rejecting applications, submitting them for approval, and notifying brokers based on the client's submitted application.","text_element_style":{"bold":false,"inline_code":false,"italic":false,"strikethrough":false,"underline":false}}}],"style":{"align":1,"folded":false}}},{"block_id":"doxjpuIu6yrJApnYKqYovO4SsYe","block_type":12,"bullet":{"elements":[{"text_run":{"content":"Manual Deposit Entry","text_element_style":{"bold":false,"inline_code":false,"italic":false,"strikethrough":false,"underline":false}}}],"style":{"align":1,"folded":false}},"parent_id":"K3qdd1XEoor1t9xRePkjJNI8pah"},{"block_id":"doxjpohDXghpQ1R4RPZgkrodCWe","block_type":2,"parent_id":"K3qdd1XEoor1t9xRePkjJNI8pah","text":{"elements":[{"text_run":{"content":"This applies when a user contacts a back-office operator to manually create a transfer-in application record. The operator must sequentially enter the client's security information, account information, and the detailed security records.","text_element_style":{"bold":false,"inline_code":false,"italic":false,"strikethrough":false,"underline":false}}}],"style":{"align":1,"folded":false}}},{"block_id":"HzFVdOpIWotLbjxQJVpjcdt0p4c","block_type":27,"image":{"align":2,"height":570,"scale":1,"token":"OLmjbiLzroUchex3D9Djx3jTpSh","width":1280},"parent_id":"K3qdd1XEoor1t9xRePkjJNI8pah"},{"block_id":"CCEtdjXD6otNudx0fcDjTAnYpZe","block_type":2,"parent_id":"K3qdd1XEoor1t9xRePkjJNI8pah","text":{"elements":[{"text_run":{"content":"","text_element_style":{"bold":false,"inline_code":false,"italic":false,"strikethrough":false,"underline":false}}}],"style":{"align":1,"folded":false}}},{"block_id":"doxjpJ5MBTBc09X7QVvPUa91Ldv","block_type":12,"bullet":{"elements":[{"text_run":{"content":"If the transferred shares are physical stock, select \"Is Physical Stock\" = Yes.","text_element_style":{"bold":false,"inline_code":false,"italic":false,"strikethrough":false,"underline":false}}}],"style":{"align":1,"folded":false}},"parent_id":"K3qdd1XEoor1t9xRePkjJNI8pah"},{"block_id":"doxjpTdKU2NCQ2Lmj2kjtZyYTBe","block_type":12,"bullet":{"elements":[{"text_run":{"content":"Reject (bulk operation available): If the client's securities deposit application contains errors or the user indicates the application is invalid, the operator may perform the [Reject] action.","text_element_style":{"bold":false,"inline_code":false,"italic":false,"strikethrough":false,"underline":false}}}],"style":{"align":1,"folded":false}},"parent_id":"K3qdd1XEoor1t9xRePkjJNI8pah"},{"block_id":"doxjplT2cu4dCrNjxgLXjRQ5fBd","block_type":12,"bullet":{"elements":[{"text_run":{"content":"Notify Broker: Because transfer-in operations require cooperation from the counterparty broker, users typically contact the broker to ensure timely handling. This function provides an email-sending capability and displays to the user whether the broker has been notified based on the email delivery results, enabling the user to proceed with subsequent business processing.","text_element_style":{"bold":false,"inline_code":false,"italic":false,"strikethrough":false,"underline":false}}}],"style":{"align":1,"folded":false}},"children":["doxjp4Yn4BC0vjeaLFco0g1vSml","doxjpLsqhvuAVBOvcujb4uQ7rLc","doxjpb1iAXxeet7ssaZAS3EcZRd","doxjpip5VFt4It6wl7JqW342PEb","F5jwdC8CKoislyxoFonjACZHprh","OdrTdAp3qoZ9OExnhRajiJk1prS","HDVtd2bqmoivqVx1rWcjj1ecpfb","K3rfd68jXojgwixKQEmju9pvppc"],"parent_id":"K3qdd1XEoor1t9xRePkjJNI8pah"},{"block_id":"doxjp4Yn4BC0vjeaLFco0g1vSml","block_type":12,"bullet":{"elements":[{"text_run":{"content":"Select the broker institutions to which the email should be sent; the system will automatically populate the broker's email address for the operator to verify.","text_element_style":{"bold":false,"inline_code":false,"italic":false,"strikethrough":false,"underline":false}}}],"style":{"align":1,"folded":false}},"parent_id":"doxjplT2cu4dCrNjxgLXjRQ5fBd"},{"block_id":"doxjpLsqhvuAVBOvcujb4uQ7rLc","block_type":12,"bullet":{"elements":[{"text_run":{"content":"Select the securities deposit application details related to the target broker (applications that have already been sent will be excluded).","text_element_style":{"bold":false,"inline_code":false,"italic":false,"strikethrough":false,"underline":false}}}],"style":{"align":1,"folded":false}},"parent_id":"doxjplT2cu4dCrNjxgLXjRQ5fBd"},{"block_id":"doxjpb1iAXxeet7ssaZAS3EcZRd","block_type":12,"bullet":{"elements":[{"text_run":{"content":"Preview the email content and, after confirming accuracy, click Send.","text_element_style":{"bold":false,"inline_code":false,"italic":false,"strikethrough":false,"underline":false}}}],"style":{"align":1,"folded":false}},"parent_id":"doxjplT2cu4dCrNjxgLXjRQ5fBd"},{"block_id":"doxjpip5VFt4It6wl7JqW342PEb","block_type":12,"bullet":{"elements":[{"text_run":{"content":"View the sending status on the Email Sending Records page.","text_element_style":{"bold":false,"inline_code":false,"italic":false,"strikethrough":false,"underline":false}}}],"style":{"align":1,"folded":false}},"parent_id":"doxjplT2cu4dCrNjxgLXjRQ5fBd"},{"block_id":"F5jwdC8CKoislyxoFonjACZHprh","block_type":27,"image":{"align":2,"height":572,"scale":1,"token":"StcGbnisloXhnqxNXbcjBmx6p9k","width":1280},"parent_id":"doxjplT2cu4dCrNjxgLXjRQ5fBd"},{"block_id":"OdrTdAp3qoZ9OExnhRajiJk1prS","block_type":27,"image":{"align":2,"height":567,"scale":1,"token":"VIImb0HRMoP4xdxghzWjfcZEpRb","width":1280},"parent_id":"doxjplT2cu4dCrNjxgLXjRQ5fBd"},{"block_id":"HDVtd2bqmoivqVx1rWcjj1ecpfb","block_type":27,"image":{"align":2,"height":1310,"scale":1,"token":"X31jbm0siofGfPxyF8ijugQgpOc","width":3022},"parent_id":"doxjplT2cu4dCrNjxgLXjRQ5fBd"},{"block_id":"K3rfd68jXojgwixKQEmju9pvppc","block_type":27,"image":{"align":2,"height":1322,"scale":1,"token":"LfNybNyQIoqNpuxamrqjVvYrpLh","width":2964},"parent_id":"doxjplT2cu4dCrNjxgLXjRQ5fBd"},{"block_id":"doxjpH28kmHN30eMMpucxkZo8ab","block_type":12,"bullet":{"elements":[{"text_run":{"content":"Submit (bulk operation available): After the operator's preliminary review confirms correctness, the application may be submitted to the next-node operator for review — the deposit entry operation.","text_element_style":{"bold":false,"inline_code":false,"italic":false,"strikethrough":false,"underline":false}}}],"style":{"align":1,"folded":false}},"parent_id":"K3qdd1XEoor1t9xRePkjJNI8pah"},{"block_id":"doxjpaEBOTE0yO2fSnvSARSWZvG","block_type":12,"bullet":{"elements":[{"text_run":{"content":"Edit: If, during processing the transfer application, the operator learns from the client whether the broker has been notified or not, the operator can use the [Edit] button to modify the notification status so that the broker's awareness matches the actual situation.","text_element_style":{"bold":false,"inline_code":false,"italic":false,"strikethrough":false,"underline":false}}}],"style":{"align":1,"folded":false}},"parent_id":"K3qdd1XEoor1t9xRePkjJNI8pah"},{"block_id":"FVEtdA6igoJx3fxYwHEjsSMipJb","block_type":27,"image":{"align":2,"height":1346,"scale":1,"token":"SHfDbwsAnopDDex3i22jLrk4pvd","width":2954},"parent_id":"K3qdd1XEoor1t9xRePkjJNI8pah"},{"block_id":"doxjpRSgtxdD6KEnPzQX50Q37Hg","block_type":12,"bullet":{"elements":[{"text_run":{"content":"Metric Cards","text_element_style":{"bold":false,"inline_code":false,"italic":false,"strikethrough":false,"underline":false}}}],"style":{"align":1,"folded":false}},"parent_id":"K3qdd1XEoor1t9xRePkjJNI8pah"},{"block_id":"doxjpn06BevrBE0GDRhWoRnQItb","block_type":2,"parent_id":"K3qdd1XEoor1t9xRePkjJNI8pah","text":{"elements":[{"text_run":{"content":"If a broker's securities transfer-in volume is high, the system's categorized display cards can be used for more granular operations. The left-to-right arrangement of the metric cards reflects the sequence of business operations.","text_element_style":{"bold":false,"inline_code":false,"italic":false,"strikethrough":false,"underline":false}}}],"style":{"align":1,"folded":false}}},{"block_id":"R9ojdGQ1xojqetxMFYnjBgE1prf","block_type":27,"image":{"align":2,"height":1330,"scale":1,"token":"KNJObwgmpoO5RyxHbQ2jvIHppFT","width":2988},"parent_id":"K3qdd1XEoor1t9xRePkjJNI8pah"},{"block_id":"doxjpCUQtmpdmertxEVnYp4UmIb","block_type":5,"heading3":{"elements":[{"text_run":{"content":"Deposit Details","text_element_style":{"bold":false,"inline_code":false,"italic":false,"strikethrough":false,"underline":false}}}],"style":{"align":1,"folded":false}},"parent_id":"K3qdd1XEoor1t9xRePkjJNI8pah"},{"block_id":"doxjpJGcpS9kl6FfgTUh6v0O1Xg","block_type":2,"parent_id":"K3qdd1XEoor1t9xRePkjJNI8pah","text":{"elements":[{"text_run":{"content":"Because a single deposit application may contain multiple security detail records, the deposit processing reviewer must carefully verify each deposit detail; only after approval may the deposit be entered.","text_element_style":{"bold":false,"inline_code":false,"italic":false,"strikethrough":false,"underline":false}}}],"style":{"align":1,"folded":false}}},{"block_id":"doxjpAIDglWjj0BkrPF465BHJ2b","block_type":12,"bullet":{"elements":[{"text_run":{"content":"Edit: If, during processing of security details, a specific detail record submitted by the user is found to be erroneous, it may be corrected via the [Edit] button after confirmation with the client.","text_element_style":{"bold":false,"inline_code":false,"italic":false,"strikethrough":false,"underline":false}}}],"style":{"align":1,"folded":false}},"children":["CkMRdBHEToS3vcxklzKjopq9pPt","doxjpXjp1v3p8757HgQXZxQJ1Je"],"parent_id":"K3qdd1XEoor1t9xRePkjJNI8pah"},{"block_id":"CkMRdBHEToS3vcxklzKjopq9pPt","block_type":27,"image":{"align":2,"height":1244,"scale":1,"token":"VC4rb7uD0ofUnOx7vqNjo7Mspyf","width":3002},"parent_id":"doxjpAIDglWjj0BkrPF465BHJ2b"},{"block_id":"doxjpXjp1v3p8757HgQXZxQJ1Je","block_type":12,"bullet":{"elements":[{"text_run":{"content":"Editable fields: stock name, custodian code, sub-account number, security detail quantity, transfer cost, and settlement remarks.","text_element_style":{"bold":false,"inline_code":false,"italic":false,"strikethrough":false,"underline":false}}}],"style":{"align":1,"folded":false}},"parent_id":"doxjpAIDglWjj0BkrPF465BHJ2b"},{"block_id":"doxjpkp4EW129yCZx1RQzbqUk4y","block_type":12,"bullet":{"elements":[{"text_run":{"content":"Delete: If the user indicates that a specific submitted detail will no longer be transferred, the target detail record can be removed via the [Delete] button.","text_element_style":{"bold":false,"inline_code":false,"italic":false,"strikethrough":false,"underline":false}}}],"style":{"align":1,"folded":false}},"parent_id":"K3qdd1XEoor1t9xRePkjJNI8pah"},{"block_id":"doxjpqpdrmYWvnB4UaJriSL6Plg","block_type":12,"bullet":{"elements":[{"text_run":{"content":"Generate SI Instructions (bulk operation available): Because transfer-in operations require necessary SI information to be entered in CCASS, the \"Generate SI Instructions\" function can automatically save this required information as a file for users to upload to CCASS, avoiding manual entry in the CCASS system. Generated SI instructions can also be viewed and exported under the \"SI Instruction List\" menu.","text_element_style":{"bold":false,"inline_code":false,"italic":false,"strikethrough":false,"underline":false}}}],"style":{"align":1,"folded":false}},"parent_id":"K3qdd1XEoor1t9xRePkjJNI8pah"},{"block_id":"NlWNdWfEvoxjbqxj1GqjcKp0ptc","block_type":27,"image":{"align":2,"height":1236,"scale":1,"token":"Fb75boLeeoJGiNxEDSijWuvDpUg","width":2992},"parent_id":"K3qdd1XEoor1t9xRePkjJNI8pah"},{"block_id":"Hd62dftXno2cnkx3Cx7jI4jLpBd","block_type":27,"image":{"align":2,"height":1304,"scale":1,"token":"GWvjba7LIouY6oxpKQZjKddnpvg","width":2970},"parent_id":"K3qdd1XEoor1t9xRePkjJNI8pah"},{"block_id":"doxjpu4AQnL0fOpmOt52I3a3rlb","block_type":12,"bullet":{"elements":[{"text_run":{"content":"Edit SI Instructions (bulk operation available): If modifications are required to generated SI information, select [Edit SI Info] to correct the target fields.","text_element_style":{"bold":false,"inline_code":false,"italic":false,"strikethrough":false,"underline":false}}}],"style":{"align":1,"folded":false}},"parent_id":"K3qdd1XEoor1t9xRePkjJNI8pah"},{"block_id":"HSwYdapHZoJLI3xJQKsjdrjipWe","block_type":27,"image":{"align":2,"height":1306,"scale":1,"token":"Y4NobzIZ8oPYy5xhCs0jcfTOphh","width":2990},"parent_id":"K3qdd1XEoor1t9xRePkjJNI8pah"},{"block_id":"doxjpN4ARz5cG09dqIgcHgIurQe","block_type":12,"bullet":{"elements":[{"text_run":{"content":"Process — Successful Submission / Failed Submission (bulk operation available): If the operator determines that a detail has no issues, they may select [Process] and choose \"Successful Submission\"; otherwise, choose \"Failed Submission\".","text_element_style":{"bold":false,"inline_code":false,"italic":false,"strikethrough":false,"underline":false}}}],"style":{"align":1,"folded":false}},"children":["BIi6d9HAGo5WGkx1wDZjiYpephf","HEgzdnGpko8ntdxbd9jjZBt0pEf","doxjp7XJB4qUeJ4ALKzU8UuV4wf","doxjpie38nxTnMgQeXUa8m78zQh"],"parent_id":"K3qdd1XEoor1t9xRePkjJNI8pah"},{"block_id":"BIi6d9HAGo5WGkx1wDZjiYpephf","block_type":27,"image":{"align":2,"height":446,"scale":1,"token":"Ub60bzRAboB4fTxy8nXjkAvWpuc","width":1280},"parent_id":"doxjpN4ARz5cG09dqIgcHgIurQe"},{"block_id":"HEgzdnGpko8ntdxbd9jjZBt0pEf","block_type":27,"image":{"align":2,"height":641,"scale":1,"token":"Er8vbezLzoWIOpxUzncjdAZgpVe","width":1280},"parent_id":"doxjpN4ARz5cG09dqIgcHgIurQe"},{"block_id":"doxjp7XJB4qUeJ4ALKzU8UuV4wf","block_type":12,"bullet":{"elements":[{"text_run":{"content":"After processing, the workflow proceeds to the next node and requires review in the Work Order Center; once approved, the stock is successfully deposited.","text_element_style":{"bold":false,"inline_code":false,"italic":false,"strikethrough":false,"underline":false}}}],"style":{"align":1,"folded":false}},"parent_id":"doxjpN4ARz5cG09dqIgcHgIurQe"},{"block_id":"doxjpie38nxTnMgQeXUa8m78zQh","bl