rough":false,"underline":false}}}],"style":{"align":1,"folded":false}},"children":["doxjpbGI7NJw1U5gYsAmvE9Skyg","doxjp0PEXe00kKNIFvVU5LSIMSh","doxjptgoJbpV3L8UuzJ82aq66he","doxjpPyDZy0gvNPgl07V4wVoyDg"],"parent_id":"UmdmdnjUgoHmN8xLkXNj74gKpVh"},{"block_id":"doxjpbGI7NJw1U5gYsAmvE9Skyg","block_type":12,"bullet":{"elements":[{"text_run":{"content":"Feature Overview:","text_element_style":{"bold":true,"inline_code":false,"italic":false,"strikethrough":false,"underline":false}}},{"text_run":{"content":" This component enables traders to access, in a single location, detailed information on customer assets, historical transactions, agency relationships, and account activity.","text_element_style":{"bold":false,"inline_code":false,"italic":false,"strikethrough":false,"underline":false}}}],"style":{"align":1,"folded":false}},"parent_id":"doxjpsCiKPoRtoQVATyrjkPGBgf"},{"block_id":"doxjp0PEXe00kKNIFvVU5LSIMSh","block_type":12,"bullet":{"elements":[{"text_run":{"content":"Customer Profile Insights:","text_element_style":{"bold":true,"inline_code":false,"italic":false,"strikethrough":false,"underline":false}}},{"text_run":{"content":" Traders can quickly obtain detailed information about customer agents and authorized persons through the component, providing necessary context for trading decisions.","text_element_style":{"bold":false,"inline_code":false,"italic":false,"strikethrough":false,"underline":false}}}],"style":{"align":1,"folded":false}},"parent_id":"doxjpsCiKPoRtoQVATyrjkPGBgf"},{"block_id":"doxjptgoJbpV3L8UuzJ82aq66he","block_type":12,"bullet":{"elements":[{"text_run":{"content":"Trading Authorization Monitoring:","text_element_style":{"bold":true,"inline_code":false,"italic":false,"strikethrough":false,"underline":false}}},{"text_run":{"content":" The component provides real-time updates on customers' trading authorization status, helping traders assess customers' current trading capabilities.","text_element_style":{"bold":false,"inline_code":false,"italic":false