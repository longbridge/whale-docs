nline_code":false,"italic":false,"strikethrough":false,"underline":false}}}],"style":{"align":1,"folded":false}}},{"block_id":"doxjpBCseHnyiDJ8c7EBQSFraDd","block_type":2,"parent_id":"RNYwd7ZslozvuIxU9IvjGFhOpsf","text":{"elements":[{"text_run":{"content":"好處在於，用戶可以以單元組件拼出自己需要的場景，或者在脫離主窗口的情況下，單元組件同樣能夠發揮出價值或作用，比如在看新聞資訊的時候拖個自選列表或者個股的圖表放旁邊同時觀看。","text_element_style":{"bold":false,"inline_code":false,"italic":false,"strikethrough":false,"underline":false}}}],"style":{"align":1,"folded":false}}},{"block_id":"doxjpbnrwuXLnLgpWlblZP6gwpc","block_type":27,"image":{"align":2,"height":1740,"token":"Y8vCbvXhnoaCCvxtTMGjapm1pog","width":2868},"parent_id":"RNYwd7ZslozvuIxU9IvjGFhOpsf"},{"block_id":"doxjpPbYLVVCGHJaquGaYLTzoMh","block_type":4,"heading2":{"elements":[{"text_run":{"content":"1.2 業務組件","text_element_style":{"bold":false,"inline_code":false,"italic":false,"strikethrough":false,"underline":false}}}],"style":{"align":1,"folded":false}},"parent_id":"RNYwd7ZslozvuIxU9IvjGFhOpsf"},{"block_id":"doxjpGa102sTeQ7ErzrdHJsN9le","block_type":12,"bullet":{"elements":[{"text_run":{"content":"業務組件是基於多個單元組件組合，承載某個業務的組件。","text_element_style":{"bold":false,"inline_code":false,"italic":false,"strikethrough":false,"underline":false}}}],"style":{"align":1,"folded":false}},"parent_id":"RNYwd7ZslozvuIxU9IvjGFhOpsf"},{"block_id":"doxjpE8whV8Awhk3IDpbQwdwc9t","block_type":12,"bullet":{"elements":[{"text_run":{"content":"業務組件的好處在於業務組件能夠滿足大多數場景下，用戶需要添加一個組件就能滿足目的，而不是一次一次添加多個。","text_element_style":{"bold":false,"inline_code":false,"italic":false,"strikethrough":false,"underline":false}}}],"style":{"align":1,"folded":false}},"parent_id":"RNYwd7ZslozvuIxU9IvjGFhOpsf"},{"block_id":"doxjp7CsvSAa8rfPgSzpzjKV7db","block_type":12,"bullet":{"elements":[{"text_run":{"content":"如迷你報價為業務組件，集合了行情報價、逐筆成交、買賣盤口和經紀席位功能。","text_element_style":{"bold":false,"inline_code":false,"italic":false,"strikethrough":false,"underline":false}}}],"style":{"align":1,"folded":false}},"parent_id":"RNYwd7ZslozvuIxU9IvjGFhOpsf"