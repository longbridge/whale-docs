ine_code":false,"italic":false,"strikethrough":false,"underline":false}}}],"style":{"align":1,"folded":false}},"parent_id":"TVGsdcye1ov5vvxnuaCjmstpp5k"},{"block_id":"doxjpYyWdhFLKmqJh2a2DUZ9aZe","block_type":3,"heading1":{"elements":[{"text_run":{"content":"Updates","text_element_style":{"bold":false,"inline_code":false,"italic":false,"strikethrough":false,"underline":false}}}],"style":{"align":1,"folded":false}},"parent_id":"TVGsdcye1ov5vvxnuaCjmstpp5k"},{"block_id":"doxjp4QTnSr5456E5Em0Ny2ek4g","block_type":2,"parent_id":"TVGsdcye1ov5vvxnuaCjmstpp5k","text":{"elements":[{"text_run":{"content":"2024.09.05 Initial Draft","text_element_style":{"bold":false,"inline_code":false,"italic":false,"strikethrough":false,"underline":false}}}],"style":{"align":1,"folded":false}}},{"block_id":"doxjpgpn85tsKmMmliSRjcU2VTe","block_type":3,"heading1":{"elements":[{"text_run":{"content":"Feature Description","text_element_style":{"bold":false,"inline_code":false,"italic":false,"strikethrough":false,"underline":false}}}],"style":{"align":1,"folded":false}},"parent_id":"TVGsdcye1ov5vvxnuaCjmstpp5k"},{"block_id":"doxjpKAc66Z8y1SCiQYWN9TnNHf","block_type":4,"heading2":{"elements":[{"text_run":{"content":"1. Market Charts","text_element_style":{"bold":false,"inline_code":false,"italic":false,"strikethrough":false,"underline":false}}}],"style":{"align":1,"folded":false}},"parent_id":"TVGsdcye1ov5vvxnuaCjmstpp5k"},{"block_id":"doxjptOPr6WvBNgN1uhz7qf9DSb","block_type":2,"parent_id":"TVGsdcye1ov5vvxnuaCjmstpp5k","text":{"elements":[{"text_run":{"content":"Market charts are used to view intraday and K-line (candlestick) data, support technical indicator overlays and drawing assistance functions, and offer two modes: Classic mode and TradingView mode.","text_element_style":{"bold":false,"inline_code":false,"italic":false,"strikethrough":false,"underline":false}}}],"style":{"align":1,"folded":false}}},{"block_id":"doxjpC1wydhE8xiChNR76CCQoUG","block_type":4,"heading2":{"elements":[{"text_run":{"content":"1.1 Classic Mode","text_element_style":{"bold":false,"inline_code":false,"italic":false,"strikethrough":false,"underline":false}}}],"style":{"align":1,"folded":false}},"parent_id":"TVGsdcye1ov5vvxnuaCjmstpp5k"},{"block_id":"doxjpLtElwjvVC7bZlRlj0iEsNe","block_type":12,"bullet":{"elements":[{"text_run":{"content":"Intraday Chart:","text_element_style":{"bold":false,"inline_code":false,"italic":false,"strikethrough":false,"underline":false}}}],"style":{"align":1,"folded":false}},"parent_id":"TVGsdcye1ov5vvxnuaCjmstpp5k"},{"block_id":"doxjpAFTCmqJNiTh9S1oHQxb5jX","block_type":2,"parent_id":"TVGsdcye1ov5vvxnuaCjmstpp5k","text":{"elements":[{"text_run":{"content":"Displays different content depending on the market. For example, for U.S. equities, it provides five intraday states for user selection: night session intraday, pre-market intraday, regular session intraday, post-market intraday, and full-day intraday. Indicator support includes four intraday indicators: MACD, Volume, RSI, and KDJ.","text_element_style":{"bold":false,"inline_code":false,"italic":false,"strikethrough":false,"underline":false}}}],"style":{"align":1,"folded":false}}},{"block_id":"doxjpYXmsLGGRkV9LkAdzt9HeOe","block_type":12,"bullet":{"elements":[{"text_run":{"content":"K-line Chart (Candlestick Chart):","text_element_style":{"bold":false,"inline_code":false,"italic":false,"strikethrough":false,"underline":false}}}],"style":{"align":1,"folded":false}},"parent_id":"TVGsdcye1ov5vvxnuaCjmstpp5k"},{"block_id":"doxjpOrbQ4aD1gzaQXwgzSEgT2b","block_type":2,"parent_id":"TVGsdcye1ov5vvxnuaCjmstpp5k","text":{"elements":[{"text_run":{"content":"Offers multi-dimensional timeframe selection, ranging from minute-level to daily and annual K-lines. Primary interactions include dragging to view additional historical K-lines, zooming to adjust candlestick size, and using the crosshair to inspect detailed information for a single candlestick, such as open price, close price, price change, and volume.","text_element_style":{"bold":false,"inline_code":false,"italic":false,"strikethrough":false,"underline":false}}}],"style":{"align":1,"folded":false}}},{"block_id":"doxjpq5bgXqFJJ3Cni7zKiPwnRg","block_type":12,"bullet":{"elements":[{"text_run":{"content":"Indicator Loading:","text_element_style":{"bold":false,"inline_code":false,"italic":false,"strikethrough":false,"underline":false}}}],"style":{"align":1,"folded":false}},"parent_id":"TVGsdcye1ov5vvxnuaCjmstpp5k"},{"block_id":"doxjpaLCqy3pZMB1Fw345Kh6nQh","block_type":2,"parent_id":"TVGsdcye1ov5vvxnuaCjmstpp5k","text":{"elements":[{"text_run":{"content":"The entry is located at the top-right of the main chart; an indicator loading popup appears to the right of the timeframe switch button. We provide a variety of indicator types for the main chart, subcharts, and intraday subcharts.","text_element_style":{"bold":false,"inline_code":false,"italic":false,"strikethrough":false,"underline":false}}}],"style":{"align":1,"folded":false}}},{"block_id":"doxjpIhhTZrTfCGTTEYffo4nkTf","block_type":2,"parent_id":"TVGsdcye1ov5vvxnuaCjmstpp5k","text":{"elements":[{"text_run":{"content":"Users can control the loading of each indicator on the main chart via the toggle behind each indicator. After clicking an indicator, the right side will display two tabs: \"Overview\" and \"Indicator Settings.\" Users may adjust parameters and toggle indicator-line visibility in \"Indicator Settings.\"","text_element_style":{"bold":false,"inline_code":false,"italic":false,"strikethrough":false,"underline":false}}}],"style":{"align":1,"folded":false}}},{"block_id":"doxjp34slo2TUwh2fsK57d8pvVg","block_type":2,"parent_id":"TVGsdcye1ov5vvxnuaCjmstpp5k","text":{"elements":[{"text_run":{"content":"Loaded indicators appear on the main chart or subcharts. Click the arrow behind an indicator to remove it, or click the \"Indicator Settings\" button to open the settings popup for quick adjustments.","text_element_style":{"bold":false,"inline_code":false,"italic":false,"strikethrough":false,"underline":false}}}],"style":{"align":1,"folded":false}}},{"block_id":"doxjpOuKhJhfzfNJSUl2zRGHAeg","block_type":12,"bullet":{"elements":[{"text_run":{"content":"Drawing Tools:","text_element_style":{"bold":false,"inline_code":false,"italic":false,"strikethrough":false,"underline":false}}}],"style":{"align":1,"folded":false}},"parent_id":"TVGsdcye1ov5vvxnuaCjmstpp5k"},{"block_id":"doxjp6N75xdcIjrFtJ4SoOZEs9d","block_type":2,"parent_id":"TVGsdcye1ov5vvxnuaCjmstpp5k","text":{"elements":[{"text_run":{"content":"Click the button next to the indicator loader to enable drawing mode; a variety of drawing tools will appear above the main chart. Users can select the desired drawing type and operate directly on the chart. The drawing tools also provide trade statistics, continuous drawing, delete, and hide functions to facilitate flexible use.","text_element_style":{"bold":false,"inline_code":false,"italic":false,"strikethrough":false,"underline":false}}}],"style":{"align":1,"folded":false}}},{"block_id":"doxjpSh9W0dRrmQ6oLS582QY9ye","block_type":12,"bullet":{"elements":[{"text_run":{"content":"Range Statistics:","text_element_style":{"bold":false,"inline_code":false,"italic":false,"strikethrough":false,"underline":false}}}],"style":{"align":1,"folded":false}},"parent_id":"TVGsdcye1ov5vvxnuaCjmstpp5k"},{"block_id":"doxjpMKzxLQ4242tYUE70R0nXjd","block_type":2,"parent_id":"TVGsdcye1ov5vvxnuaCjmstpp5k","text":{"elements":[{"text_run":{"content":"The range statistics feature is located within the drawing tools. After clicking the drawing button, the trade-statistics toggle can be found in the center of the toolbar. When enabled, a statistical-range popup appears on the main chart, and users can determine the range by dragging the left and right boundaries.","text_element_style":{"bold":false,"inline_code":false,"italic":false,"strikethrough":false,"underline":false}}}],"style":{"align":1,"folded":false}}},{"block_id":"doxjpQUSNo2V9WH8C1c5Q79M0Qg","block_type":2,"parent_id":"TVGsdcye1ov5vvxnuaCjmstpp5k","text":{"elements":[{"text_run":{"content":"The statistics popup displays the metrics for the selected period; users can also adjust the time range using the arrows next to the start and end times in the popup.","text_element_style":{"bold":false,"inline_code":false,"italic":false,"strikethrough":false,"underline":false}}}],"style":{"align":1,"folded":false}}},{"block_id":"doxjpN8xghnCCyC1x4tsV8D88od","block_type":4,"heading2":{"elements":[{"text_run":{"content":"1.2 TradingView Mode","text_element_style":{"bold":false,"inline_code":false,"italic":false,"strikethrough":false,"underline":false}}}],"style":{"align":1,"folded":false}},"parent_id":"TVGsdcye1ov5vvxnuaCjmstpp5k"},{"block_id":"doxjp29LflXsWwWk4dzAnYsCXFc","block_type":2,"parent_id":"TVGsdcye1ov5vvxnuaCjmstpp5k","text":{"elements":[{"text_run":{"content":"Embeds TradingView’s open-source charts to provide an alternative chart presentation, supporting more detailed settings such as main chart background color, axes, and other visual details. Its drawing tools also include unique features that enhance the user experience.","text_element_style":{"bold":false,"inline_code":false,"italic":false,"strikethrough":false,"underline":false}}}],"style":{"align":1,"folded":false}}},{"block_id":"doxjpjRQDGygmZ2tKBPBVlCANqh","block_type":4,"heading2":{"elements":[{"text_run":{"content":"2. Market Quote Component","text_element_style":{"bold":false,"inline_code":false,"italic":false,"strikethrough":false,"underline":false}}}],"style":{"align":1,"folded":false}},"parent_id":"TVGsdcye1ov5vvxnuaCjmstpp5k"},{"block_id":"doxjp3UPJgRJsX0SNgnVkTBqfcH","block_type":2,"parent_id":"TVGsdcye1ov5vvxnuaCjmstpp5k","text":{"elements":[{"text_run":{"content":"The market quote component displays real-time quote information for individual securities, with fields that vary by instrument type. The component shows the instrument name and trading status, and uses icons to indicate market and permission information. The heart button next to the instrument name can be used to add or remove the instrument from the watchlist.","text_element_style":{"bold":false,"inline_code":false,"italic":false,"strikethrough":false,"underline":false}}}],"style":{"align":1,"folded":false}}},{"block_id":"doxjpIpogidirf3ciQWHg7TpkKH","block_type":4,"heading2":{"elements":[{"text_run":{"content":"3. Order Book Component","text_element_style":{"bold":false,"inline_code":false,"italic":false,"strikethrough":false,"underline":false}}}],"style":{"align":1,"folded":false}},"parent_id":"TVGsdcye1ov5vvxnuaCjmstpp5k"},{"block_id":"doxjp7Ck2D6h3gQhp63gja3Btwb","block_type":2,"parent_id":"TVGsdcye1ov5vvxnuaCjmstpp5k","text":{"elements":[{"text_run":{"content":"The order book displays the instrument’s current order placements, and the content varies according to user permissions. For U.S. equities, different permissions may reveal single-level bids, multi-level bids, or full depth. Time-sequenced trade data presents the day’s transactions in chronological order.","text_element_style":{"bold":false,"inline_code":false,"italic":false,"strikethrough":false,"underline":false}}}],"style":{"align":1,"folded":false}}},{"block_id":"doxjpdtbVHz9ctzFles0e1OHDsf","block_type":4,"heading2":{"elements":[{"text_run":{"content":"4. Broker Quotes","text_element_style":{"bold":false,"inline_code":false,"italic":false,"strikethrough":false,"underline":false}}}],"style":{"align":1,"folded":false}},"parent_id":"TVGsdcye1ov5vvxnuaCjmstpp5k"},{"block_id":"doxjp6JcSsJK4gn8iK46FLe5Xyf","block_type":2,"parent_id":"TVGsdcye1ov5vvxnuaCjmstpp5k","text":{"elements":[{"text_run":{"content":"Broker quotes is a module specific to Hong Kong stocks, available to users with Level 2 or higher permissions. This feature displays each broker’s quotes at different price levels.","text_element_style":{"bold":false,"inline_code":false,"italic":false,"strikethrough":false,"underline":false}}}],"style":{"align":1,"folded":false}}},{"block_id":"doxjpAPUttlogrUFhuvrNDGxJ3b","block_type":4,"heading2":{"elements":[{"text_run":{"content":"5. Option Chain","text_element_style":{"bold":false,"inline_code":false,"italic":false,"strikethrough":false,"underline":false}}}],"style":{"align":1,"folded":false}},"parent_id":"TVGsdcye1ov5vvxnuaCjmstpp5k"},{"block_id":"doxjp3NiviEGzIuCK0VXePB9seh","block_type":2,"parent_id":"TVGsdcye1ov5vvxnuaCjmstpp5k","text":{"elements":[{"text_run":{"content":"The option chain is an important feature for U.S. equities, presenting option quotes across strikes and expiration dates. Users can customize the option chain layout by showing or hiding fields, adjusting sort order, and selecting option types to create the desired view.","text_element_style":{"bold":false,"inline_code":false,"italic":false,"strikethrough":false,"underline":false}}}],"style":{"align":1,"folded":false}}},{"block_id":"doxjp84e6f6h6OsI2YTj5jcd6Qe","block_type":4,"heading2":{"elements":[{"text_run":{"content":"6. Stock Screener","text_element_style":{"bold":false,"inline_code":false,"italic":false,"strikethrough":false,"underline":false}}}],"style":{"align":1,"folded":false}},"parent_id":"TVGsdcye1ov5vvxnuaCjmstpp5k"},{"block_id":"doxjpvQYBfuReY4s9MiBlpUO7k2","block_type":2,"parent_id":"TVGsdcye1ov5vvxnuaCjmstpp5k","text":{"elements":[{"text_run":{"content":"The stock screener is accessed from the tab bar at the top of the application; click it to enter the screener interface. First, click \"Filter Criteria\" to open the filter-criteria popup. Users can select desired filter criteria; clicking a criterion adds it to the filter list on the left side of the screen. Users may adjust criterion values by entering precise numbers or dragging the range sliders below. After confirming the criteria, the \"Matches\" area displays the number of stocks that meet the conditions. The list of stocks satisfying all filter criteria is shown to the right of the filter list.","text_element_style":{"bold":false,"inline_code":false,"italic":false,"strikethrough":false,"underline":false}}}],"style":{"align":1,"folded":false}}}]}