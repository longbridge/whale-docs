gh":false,"underline":false}}}],"style":{"align":1,"folded":false}}},{"block_id":"doxjp0Viwo3mFr7IhEU5NKaMAQb","block_type":27,"image":{"align":2,"height":1418,"token":"VGKYbuFYxoAPTUxOtDzj61LYp33","width":2858},"parent_id":"Lq2cdsCBLo80ITxRWEdjRnYTpHf"},{"block_id":"doxjp66ZK83vNQFkGPU87TMXecc","block_type":2,"parent_id":"Lq2cdsCBLo80ITxRWEdjRnYTpHf","text":{"elements":[{"text_run":{"content":"Figure 2C-1","text_element_style":{"bold":false,"inline_code":false,"italic":true,"strikethrough":false,"underline":false}}}],"style":{"align":1,"folded":false}}},{"block_id":"doxjpe6WHiatimQKK5Ra70d6WYc","block_type":27,"image":{"align":2,"height":1420,"token":"AlMhbvggXouih9xgZoQjnJmkpMg","width":2390},"parent_id":"Lq2cdsCBLo80ITxRWEdjRnYTpHf"},{"block_id":"doxjpxV1ksThPUwYmofwzmngVDh","block_type":2,"parent_id":"Lq2cdsCBLo80ITxRWEdjRnYTpHf","text":{"elements":[{"text_run":{"content":"Figure 2C-2","text_element_style":{"bold":false,"inline_code":false,"italic":true,"strikethrough":false,"underline":false}}}],"style":{"align":1,"folded":false}}},{"block_id":"doxjpgdhA0WLM50SoihI6faaR3b","block_type":2,"parent_id":"Lq2cdsCBLo80ITxRWEdjRnYTpHf","text":{"elements":[{"text_run":{"content":"If interest or settlement data do not need to be recalculated, asset rollbacks may be optionally omitted.","text_element_style":{"bold":false,"inline_code":false,"italic":true,"strikethrough":false,"underline":false}}}],"style":{"align":1,"folded":false}}},{"block_id":"doxjpNwoq8UW0l2PtqTfu8ZXYmd","block_type":27,"image":{"align":2,"height":972,"token":"O64Lbg8GhoGXdoxG9vjjBp32p3e","width":2392},"parent_id":"Lq2cdsCBLo80ITxRWEdjRnYTpHf"},{"block_id":"doxjpjdb9oaWE5swtcjVpGXvgYb","block_type":2,"parent_id":"Lq2cdsCBLo80ITxRWEdjRnYTpHf","text":{"elements":[{"text_run":{"content":"Figure 2C-3","text_element_style":{"bold":false,"inline_code":false,"italic":true,"strikethrough":false,"underline":false}}}],"style":{"align":1,"folded":false}}},{"block_id":"doxjpMMBpOpWMZgCEUNfoTjKmFg","block_type":2,"parent_id":"Lq2cdsCBLo80ITxRWEdjRnYTpHf","text":{"elements":[{"text_run":{"content":"Select the \"Position Transactions / Fund Transactions\" page, choose the relevant transaction, and click \"Edit Date\".","text_element_style":{"bold":false,"inline_code":false,"italic":true,"strikethrough":false,"underline":false}}}],"style":{"align":1,"folded":false}}},{"block_id":"doxjp4f5s7SO0XW20KBU2HheDUb","block_type":3,"heading1":{"elements":[{"text_run":{"content":"2D Question: If an account has a USD deficit but an HKD balance, can HKD be withdrawn?","text_element_style":{"bold":false,"inline_code":false,"italic":false,"strikethrough":false,"underline":false}}}],"style":{"align":1,"folded":false}},"parent_id":"Lq2cdsCBLo80ITxRWEdjRnYTpHf"},{"block_id":"doxjpSJEO7JNhzFLWYKJJafszvh","block_type":2,"parent_id":"Lq2cdsCBLo80ITxRWEdjRnYTpHf","text":{"elements":[{"text_run":{"content":"Answer:","text_element_style":{"bold":true,"inline_code":false,"italic":false,"strikethrough":false,"underline":false}}},{"text_run":{"content":" In \"Business Parameter Configuration > Risk Control > Withdrawal Parameters\", click \"Edit\" on the \"Coefficient X Configuration\". Change the coefficient to 1; when the account has a USD deficit but retains purchasing power, the HKD balance may be withdrawn (see below).","text_element_style":{"bold":false,"inline_code":false,"italic":false,"strikethrough":false,"underline":false}}}],"style":{"align":1,"folded":false}}},{"block_id":"doxjpst9RoKbQoySZMlYaB5sizh","block_type":27,"image":{"align":2,"height":1438,"token":"QA5VbHEHSoidp9xPLvujad4KpYg","width":2826},"parent_id":"Lq2cdsCBLo80ITxRWEdjRnYTpHf"},{"block_id":"doxjpYPy9UNUPvMBqFcg2DV6Ilc","block_type":27,"image":{"align":2,"height":1160,"token":"GxDDbXnl9o0Zb1xnJwpjbjODpZc","width":2206},"parent_id":"Lq2cdsCBLo80ITxRWEdjRnYTpHf"},{"block_id":"doxjpyMTRv2LeeOh6a71Lp91bch","block_type":3,"heading1":{"elements":[{"text_run":{"content":"2E Question: How can different withdrawal fees be set for different currencies?","text_element_style":{"bold":false,"inline_code":false,"italic":false,"strikethrough":false,"underline":false}}}],"style":{"align":1,"folded":false}},"parent_id":"Lq2cdsCBLo80ITxRWEdjRnYTpHf"},{"block_id":"doxjpJBpcoh5JiO8H46FFCRuUcd","block_type":2,"parent_id":"Lq2cdsCBLo80ITxRWEdjRnYTpHf","text":{"elements":[{"text_run":{"content":"Answer: In \"Billing Management\", configure a fixed withdrawal fee for a specific currency by setting both the maximum fee and the minimum fee to fixed amounts.","text_element_style":{"bold":false,"inline_code":false,"italic":false,"strikethrough":false,"underline":false}}}],"style":{"align":1,"folded":false}}},{"block_id":"doxjpWGwBLWfvnCadBWlXlUaVve","block_type":27,"image":{"align":2,"height":1366,"token":"C4g1bqkQfoFBTKxyC7JjVH6QpWd","width":2226},"parent_id":"Lq2cdsCBLo80ITxRWEdjRnYTpHf"}]}