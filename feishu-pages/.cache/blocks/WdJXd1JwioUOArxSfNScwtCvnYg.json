ne":false}}},{"text_run":{"content":"：确保客户账户符合新股认购的要求，进行合规性检查。","text_element_style":{"bold":false,"inline_code":false,"italic":false,"strikethrough":false,"underline":false}}}],"style":{"align":1,"folded":false}},"parent_id":"WdJXd1JwioUOArxSfNScwtCvnYg"},{"block_id":"doxcnoDNIZzUofQYGmcAieiD5Wl","block_type":12,"bullet":{"elements":[{"text_run":{"content":"与IPO承销商协调","text_element_style":{"bold":true,"inline_code":false,"italic":false,"strikethrough":false,"underline":false}}},{"text_run":{"content":"：与承销商协调，确保准确了解配售情况和分配方案。","text_element_style":{"bold":false,"inline_code":false,"italic":false,"strikethrough":false,"underline":false}}}],"style":{"align":1,"folded":false}},"parent_id":"WdJXd1JwioUOArxSfNScwtCvnYg"},{"block_id":"doxcnb7894uCOE9kz1CjxX5elAd","block_type":12,"bullet":{"elements":[{"text_run":{"content":"后续服务和支持","text_element_style":{"bold":true,"inline_code":false,"italic":false,"strikethrough":false,"underline":false}}},{"text_run":{"content":"：提供市场信息和投资建议，解答客户的疑问。","text_element_style":{"bold":false,"inline_code":false,"italic":false,"strikethrough":false,"underline":false}}}],"style":{"align":1,"folded":false}},"parent_id":"WdJXd1JwioUOArxSfNScwtCvnYg"},{"block_id":"doxcnxLSnUqAnD7XXNTxmODhSdg","block_type":12,"bullet":{"elements":[{"text_run":{"content":"维持合规和记录保持","text_element_style":{"bold":true,"inline_code":false,"italic":false,"strikethrough":false,"underline":false}}},{"text_run":{"content":"：确保整个过程符合法规，保持透明的记录以备审计和检查。","text_element_style":{"bold":false,"inline_code":false,"italic":false,"strikethrough":false,"underline":false}}}],"style":{"align":1,"folded":false}},"parent_id":"WdJXd1JwioUOArxSfNScwtCvnYg"},{"block_id":"doxcnvzJQgELO2Rev2Eymh0xVkh","block_type":2,"parent_id":"WdJXd1JwioUOArxSfNScwtCvnYg","text":{"elements":[{"text_run":{"content":"","text_element_style":{"bold":false,"inline_code":false,"italic":false,"strikethrough":false,"underline":false}}}],"style":{"align":1,"folded":false}}}]}