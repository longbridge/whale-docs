inline_code":false,"italic":false,"strikethrough":false,"underline":false}}}],"style":{"align":1,"folded":false}}},{"block_id":"BodOdKNwSobJj1x9Ss7cJNYxnJf","block_type":27,"image":{"align":2,"height":888,"token":"Snnnb2J3qoE2jZxwi91ccEI2njf","width":2506},"parent_id":"OaGAdHvPxoLM8CxiBA7cFeFHnhc"},{"block_id":"SdtMdxfJJoAhKMxQxkjcjtmen3e","block_type":2,"parent_id":"OaGAdHvPxoLM8CxiBA7cFeFHnhc","text":{"elements":[{"text_run":{"content":"1.6A 圖一","text_element_style":{"bold":false,"inline_code":false,"italic":true,"strikethrough":false,"underline":false}}}],"style":{"align":2,"folded":false}}},{"block_id":"KGohdB9sxofjhHxWUmBcZKoWnrf","block_type":2,"parent_id":"OaGAdHvPxoLM8CxiBA7cFeFHnhc","text":{"elements":[{"text_run":{"content":"","text_element_style":{"bold":false,"inline_code":false,"italic":false,"strikethrough":false,"underline":false}}}],"style":{"align":1,"folded":false}}},{"block_id":"HpI8dnQgIopdnBxhPPKcu5qcnye","block_type":27,"image":{"align":2,"height":1348,"token":"YSlabSrgOoJ7MSxze7EcueRMnUc","width":2360},"parent_id":"OaGAdHvPxoLM8CxiBA7cFeFHnhc"},{"block_id":"SlipdrCxdoudEzxGh4TcqopBnq6","block_type":2,"parent_id":"OaGAdHvPxoLM8CxiBA7cFeFHnhc","text":{"elements":[{"text_run":{"content":"1.6A 圖二","text_element_style":{"bold":false,"inline_code":false,"italic":true,"strikethrough":false,"underline":false}}}],"style":{"align":2,"folded":false}}},{"block_id":"IuQtd6bJOortJvxPLWHcBt6OnNe","block_type":4,"heading2":{"elements":[{"text_run":{"content":"1.6B 問：主子賬户如何登錄到App？","text_element_style":{"bold":false,"inline_code":false,"italic":false,"strikethrough":false,"underline":false}}}],"style":{"align":1,"folded":false}},"parent_id":"OaGAdHvPxoLM8CxiBA7cFeFHnhc"},{"block_id":"FhHNdy8EhooQWXxxBbIcDAaEnue","block_type":2,"parent_id":"OaGAdHvPxoLM8CxiBA7cFeFHnhc","text":{"elements":[{"text_run":{"content":"答：","text_element_style":{"bold":true,"inline_code":false,"italic":false,"strikethrough":false,"underline":false}}},{"text_run":{"content":"客户如有主子賬户，也是使用同一個登記電話登入到App。登錄後可以操作到所有主子賬户作買賣及資金／股票劃轉，不能限制操作某個主／子賬户的資產。","text_element_style":{"bold":false,"inline_code":false,"italic":false,"strikethrough":false,"underline":false}}}],"style":{"align":1,"folded":false}}},{"block_id":"W3nOdy5fRoQxYPxTto6c8K2uneb","block_type":4,"heading2":{"elements":[{"text_run":{"content":"1.6C 問：為什麼在子賬户操作修改資料會失敗／沒反應？","text_element_style":{"bold":false,"inline_code":false,"italic":false,"strikethrough":false,"underline":false}}}],"style":{"align":1,"folded":false}},"parent_id":"OaGAdHvPxoLM8CxiBA7cFeFHnhc"},{"block_id":"TJkGd0THwowozBxMNB3ctWygnXg","block_type":2,"parent_id":"OaGAdHvPxoLM8CxiBA7cFeFHnhc","text":{"elements":[{"text_run":{"content":"答","text_element_style":{"bold":true,"inline_code":false,"italic":false,"strikethrough":false,"underline":false}}},{"text_run":{"content":"：主子賬户資料是共用同一份的，當其中一個主子賬户修改了，其他的賬户也會一併修改資料，如：開户資料、聯絡電話／電郵、授權人、風險評級等資料。","text_element_style":{"bold":false,"inline_code":false,"italic":false,"strikethrough":false,"underline":false}}}],"style":{"align":1,"folded":false}}},{"block_id":"PZChdVobXofolmxCgOZcSoJ6nlD","block_type":34,"children":["AB2rdWxEvoWGPFxCrcmcH3Tbnsg"],"parent_id":"OaGAdHvPxoLM8CxiBA7cFeFHnhc","quote_container":{}},{"block_id":"AB2rdWxEvoWGPFxCrcmcH3Tbnsg","block_type":2,"parent_id":"PZChdVobXofolmxCgOZcSoJ6nlD","text":{"elements":[{"text_run":{"content":"一般可以在主申請賬户中查看資料。（見1.6C 圖一）","text_element_style":{"bold":false,"inline_code":false,"italic":false,"strikethrough":false,"underline":false}}}],"style":{"align":1,"folded":false}}},{"block_id":"P75ndjlvGoHquFxeAbfcQEBgnTe","block_type":27,"image":{"align":2,"height":1326,"token":"Lt5gbqrIUoSUcaxpftYciY5AnSb","width":2508},"parent_id":"OaGAdHvPxoLM8CxiBA7cFeFHnhc"},{"block_id":"Aa7PdKRpCoC0vVxH8Dtc0UAhnre","block_type":2,"parent_id":"OaGAdHvPxoLM8CxiBA7cFeFHnhc","text":{"elements":[{"text_run":{"content":"1.6C 圖一","text_element_style":{"bold":false,"inline_code":false,"italic":true,"strikethrough":false,"underline":false}}}],"style":{"align":2,"folded":false}}},{"block_id":"B4PjdecZtohlGRxfoNscWurFnEa","block_type":4,"heading2":{"elements":[{"text_run":{"content":"1.6D 問：為什麼在「賬户列表」點擊的賬户號碼跟顯示的當前賬户號碼不一致？","text_element_style":{"bold":false,"inline_code":false,"italic":false,"strikethrough":false,"underline":false}}}],"style":{"align":1,"folded":false}},"parent_id":"OaGAdHvPxoLM8CxiBA7cFeFHnhc"},{"block_id":"G717dbVtooDlCMx4BqbcEYSPnth","block_type":2,"parent_id":"OaGAdHvPxoLM8CxiBA7cFeFHnhc","text":{"elements":[{"text_run":{"content":"答：","text_element_style":{"bold":true,"inline_code":false,"italic":false,"strikethrough":false,"underline":false}}},{"text_run":{"content":"因為客户資料實際上是同一份，不區分證券賬户，如需要查看應對證券賬户，可以通過右上角切換賬户。","text_element_style":{"bold":false,"inline_code":false,"italic":false,"strikethrough":false,"underline":false}}}],"style":{"align":1,"folded":false}}},{"block_id":"C7gmdQB42oeRQ5xeU2XcvLBGn1g","block_type":27,"image":{"align":2,"height":1004,"token":"Z6vObj51GovxJKxRDzYcBXPRnVg","width":2682},"parent_id":"OaGAdHvPxoLM8CxiBA7cFeFHnhc"},{"block_id":"OVChdreilo5U3Yxpj6lc4eusn3c","block_type":4,"heading2":{"elements":[{"text_run":{"content":"1.6E 問：公司賬戶如何操作主子賬戶","text_element_style":{"bold":false,"inline_code":false,"italic":false,"strikethrough":false,"underline":false}}}],"style":{"align":1,"folded":false}},"parent_id":"OaGAdHvPxoLM8CxiBA7cFeFHnhc"},{"block_id":"CGsPd1FOZoG3A1xP1xUcsmt5ncf","block_type":2,"parent_id":"OaGAdHvPxoLM8CxiBA7cFeFHnhc","text":{"elements":[{"text_run":{"content":"答：公司賬戶的主子賬戶登記的手機號只能為一個，不同人員需共同使用進行2fa操作；主賬户可以區分現金、融資賬户，子賬户需要跟隨主賬户。","text_element_style":{"bold":false,"inline_code":false,"italic":false,"strikethrough":false,"underline":false}}}],"style":{"align":1,"folded":false}}}]}