d":false,"inline_code":false,"italic":false,"strikethrough":false,"underline":false}}}],"style":{"align":1,"folded":false}}},{"block_id":"VXj3dEcePo0vBLxQt8pcexNvnte","block_type":2,"parent_id":"J535diUr6owFPrx2x6IcGgP4nBd","text":{"elements":[{"text_run":{"content":"例如：手機號碼是 12345678，證件號碼是 123456（X），則結單密碼是 5678456X。","text_element_style":{"bold":false,"inline_code":false,"italic":false,"strikethrough":false,"underline":false}}}],"style":{"align":1,"folded":false}}},{"block_id":"GL6odRcMiox5BMxWLwzc1zh1n1g","block_type":12,"bullet":{"elements":[{"text_run":{"content":"個人戶： 手機號碼後 4 位數字 ＋ 身分證號碼最後 4 位數字  ","text_element_style":{"bold":true,"inline_code":false,"italic":false,"strikethrough":false,"underline":false}}},{"text_run":{"content":"（ 聯名戶以主持有人為相關信息 ）","text_element_style":{"bold":false,"inline_code":false,"italic":false,"strikethrough":false,"underline":false}}}],"style":{"align":1,"folded":false}},"parent_id":"J535diUr6owFPrx2x6IcGgP4nBd"},{"block_id":"IH3DdIe3coaDdyxH532cxxP4nIh","block_type":12,"bullet":{"elements":[{"text_run":{"content":"公司户：手機號尾 4 位數字 ＋ CI 尾 4 位數字","text_element_style":{"bold":true,"inline_code":false,"italic":false,"strikethrough":false,"underline":false}}}],"style":{"align":1,"folded":false}},"parent_id":"J535diUr6owFPrx2x6IcGgP4nBd"},{"block_id":"NzLFdqeUDoVbcIxx2jtcQIt0nue","block_type":4,"heading2":{"elements":[{"text_run":{"content":"5.2B 問：沒有登錄電話，新增或更改了，會否影響到結單密碼？","text_element_style":{"bold":false,"inline_code":false,"italic":false,"strikethrough":false,"underline":false}}}],"style":{"align":1,"folded":false}},"parent_id":"J535diUr6owFPrx2x6IcGgP4nBd"},{"block_id":"US35dZHH9oW5DExK1YgcvzlgnGg","block_type":2,"parent_id":"J535diUr6owFPrx2x6IcGgP4nBd","text":{"elements":[{"text_run":{"content":"答：","text_element_style":{"bold":true,"inline_code":false,"italic":false,"strikethrough":false,"underline":false}}},{"text_run":{"content":"會影響到的，如在工單中心新增或更改了登錄手機號，結單的密碼會更新。","text_element_style":{"bold":false,"inline_code":false,"italic":false,"strikethrough":false,"underline":false}}}],"style":{"align":1,"folded":false}}},{"block_id":"ZYB9d0SVroyRTIxKETacZyD3n8c","block_type":2,"parent_id":"J535diUr6owFPrx2x6IcGgP4nBd","text":{"elements":[{"text_run":{"content":"結單的邏輯見以下：","text_element_style":{"bold":false,"inline_code":false,"italic":false,"strikethrough":false,"underline":false}}}],"style":{"align":1,"folded":false}}},{"block_id":"SLlKdeoWdoIcMQxKtyTcfAiSn7d","block_type":13,"ordered":{"elements":[{"text_run":{"content":"若客戶有登錄手機號，則附件密碼為登錄手機號後四位+身份證後四位；","text_element_style":{"bold":false,"inline_code":false,"italic":false,"strikethrough":false,"underline":false}}}],"style":{"align":1,"folded":false}},"parent_id":"J535diUr6owFPrx2x6IcGgP4nBd"},{"block_id":"XsaRdyO8qohHbPx7G39c0tO6nmH","block_type":13,"ordered":{"elements":[{"text_run":{"content":"如果没有登錄手機號，結單的密碼是用聯繫手機號後四位+身份證後四位；","text_element_style":{"bold":false,"inline_code":false,"italic":false,"strikethrough":false,"underline":false}}}],"style":{"align":1,"folded":false}},"parent_id":"J535diUr6owFPrx2x6IcGgP4nBd"},{"block_id":"WPgDdV54aoC6znxRkzccluRRneb","block_type":13,"ordered":{"elements":[{"text_run":{"content":"如沒有登錄及聯繫手機號，會取身份證後四個位。","text_element_style":{"bold":false,"inline_code":false,"italic":false,"strikethrough":false,"underline":false}}}],"style":{"align":1,"folded":false}},"parent_id":"J535diUr6owFPrx2x6IcGgP4nBd"},{"block_id":"EKQRdR0kOof7gExhJdoccf7Sn6c","block_type":2,"parent_id":"J535diUr6owFPrx2x6IcGgP4nBd","text":{"elements":[{"text_run":{"content":"如客户是提供了多個國藉證件，取的證件號邏輯順序是：香港身份證 ＞ 本國身份證 ＞ 護照，按香港交易所申報 BCAN 的順序保持一致。","text_element_style":{"bold":false,"inline_code":false,"italic":false,"strikethrough":false,"underline":false}}}],"style":{"align":1,"folded":false}}},{"block_id":"NjawdzWeRoQYhixxq4ScmZYBnEc","block_type":2,"parent_id":"J535diUr6owFPrx2x6IcGgP4nBd","text":{"elements":[{"text_run":{"content":"","text_element_style":{"bold":false,"inline_code":false,"italic":false,"strikethrough":false,"underline":false}}}],"st